<aura:component >
    <aura:attribute name="item" type="Camping_Item__c" required="true"/> 
    
     <p>Name:
        <ui:outputText value="{!v.item.Name}"/>
    </p>
    <p>Price:
        <ui:outputCurrency value="{!v.item.sinadino__Price__c}"/>
    </p>
    <p>Quantity:
        <ui:outputNumber value="{!v.item.sinadino__Quantity__c}"/>
    </p>
    <p>Packed?:
        <ui:outputCheckbox value="{!v.item.sinadino__Packed__c}"/>
    </p>
    
</aura:component>