        <availability>online</availability>
        <description>Resets fields used to track support authorization requests.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel Authorization Request</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT (&quot;/soap/ajax/25.0/connection.js&quot;)} 
{!REQUIRESCRIPT (&quot;/soap/ajax/25.0/apex.js&quot;)} 

var published = {!Case.quot__Publish_Auth_Request__c };

if (published == true) {
var item = new sforce.SObject(&quot;Case&quot;);
item.id = &quot;{!Case.Id }&quot;;
item.quot__Publish_Auth_Request__c = false;
item.quot__Request_Timestamp__c = null;

result = sforce.connection.update([item]);
window.location.reload();
}</url>
