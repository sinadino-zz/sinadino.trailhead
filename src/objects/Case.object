    <webLinks>
        <fullName>quot__Send_Authorization_Request</fullName>
        <availability>online</availability>
        <description>Checks the configuration of the case for support authorization and opens the email template used to request online approval.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send Authorization Request</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)}

var emailTemplateDeveloperName = &quot;QI_Support_Auth_Request&quot;; // This template name can be changed to any custom template you choose. Be sure to build your custom template around the Salesforce Case object.

var query = &quot;Select id from EmailTemplate where isActive = true and DeveloperName = &apos;&quot;+emailTemplateDeveloperName+&quot;&apos; &quot;;
var result = sforce.connection.query(query);
var templates = result.getArray(&quot;records&quot;);

if (templates.length == 0) {
alert(&quot;Sorry, could not find an active email template named \&quot;&quot;+emailTemplateDeveloperName+&quot;\&quot;. Please contact your Salesforce administrator for help in setting up this template.&quot;);

} else {
   // If a request was auth&apos;d before, alert user
  var continueOK = true;
  if (&quot;{!Case.quot__Auth_Payment_Processed__c}&quot; == &quot;1&quot;) {
      continueOK = confirm(&quot;A support auth has previously been approved for this case, would you like to send another?&quot;);
  }
  if (continueOK) {
      var authAmount = parseFloat(&quot;{!TEXT(Case.quot__Authorization_Amount__c)}&quot;);

      if(authAmount == NaN || authAmount == 0.0) {
         continueOK = false;
         alert(&apos;An authorization amount is required to send a support authorization request.&apos;);
      }
  }
  if (continueOK &amp;&amp; {!IF(ISNULL(Case.quot__Auth_Request_Expiration__c),&quot;blank&quot;,&quot;&quot;) == &quot;blank&quot;}) {
         continueOK = false;
         alert(&apos;Please enter an auth request expiration date before sending your support authorization request.&apos;);
  }

  if (continueOK &amp;&amp; {!IF((Case.quot__Auth_Request_Expiration__c &lt; TODAY()),&quot;expired&quot;,&quot;&quot;) == &quot;expired&quot;}) {
         continueOK = false;
         alert(&apos;This auth request has already expired.  Please enter a new expiration date.&apos;);
  }


  if (continueOK) {
    var item = new sforce.SObject(&quot;Case&quot;);
    if (&quot;{!Case.quot__Publish_Auth_Request__c}&quot; !== &quot;1&quot;) {
        // set flag, if not already set, to publish auth request.
        item.id = &quot;{!Case.Id }&quot;;
        item.quot__Publish_Auth_Request__c = true;
        result = sforce.connection.update([item]);
    }
    
    // Redirect to email template to send request.
    var templateId = templates[0].Id;
    window.location = &quot;/_ui/core/email/author/EmailAuthor?p3_lkid={!Case.Id}&amp;p2_lkid={!Case.ContactId}&amp;template_id=&quot;+templateId+&quot;&amp;cancelURL=/{!Case.Id}&amp;retURL=/{!Case.Id}&amp;new_template=1&quot;;

  }
}</url>
